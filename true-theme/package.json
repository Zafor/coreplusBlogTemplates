{
  "private": true,
  "name": "SolidBuild",
  "version": "1.0.0",
  "repository": "bitbucket:trueagency/true-theme",
  "license": "UNLICENSED",
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "lottie-web": "^5.7.3",
    "scroll-lock": "^2.1.4",
    "tailwindcss": "^1.4.6",
    "vue": "^2.6.12",
    "vue-hot-reload-api": "^2.3.4"
  },
  "scripts": {
    "tw": "npx tailwindcss build assets/css/tailwind.css -o assets/css/tailwind.min.css",
    "tw:build": "env NODE_ENV=production npx tailwindcss build assets/css/tailwind.css -o assets/css/tailwind.min.css",
    "rmdist": "rimraf dist",
    "rmcache": "rimraf .cache",
    "parcel:dev": "parcel watch assets/js/parcel.js --public-url ./ --https --hmr-hostname localhost",
    "parcel:dev-http": "parcel watch assets/js/parcel.js --public-url ./ --hmr-hostname localhost",
    "parcel:build": "rimraf .cache && parcel build assets/js/parcel.js -d assets/parcel-output/ --no-minify"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.11.5",
    "@vue/component-compiler-utils": "^3.2.0",
    "any-promise": "^1.3.0",
    "bluebird": "^3.5.0",
    "del": "^0.1.2",
    "extend": "^3.0.0",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "0.0.8",
    "gulp-babel": "^8.0.0",
    "gulp-bless": "^4.0.0",
    "gulp-clean-css": "^3.5.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^2.0.0",
    "gulp-less": "^3.3.2",
    "gulp-livereload": "^3.8.1",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.0",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.7",
    "minimist": "^1.2.0",
    "natives": "^1.1.6",
    "parcel-bundler": "^1.12.4",
    "vue-template-compiler": "^2.6.12",
    "yargs": "^3.30.0"
  }
}
